      <mapping pattern="*.json.txt" type="JSON" />
      <mapping pattern="*.lua.txt" type="lua" />
      <removed_mapping pattern=".txt" approved="true" type="lua" />
      <removed_mapping ext="txt" approved="true" type="lua" />
    </extensionMap>
  </component>
</application>